package com.thread;

class T1 extends Thread {

	String name;
	
	public T1() {}
	public T1(String name) {
		this.name = name;
	}
	
	@Override
	public void run() { //source -> override -> run

	}
}

public class Test {

	public static void main(String[] args) {
		T1 t1 = new T1("T1");
		t1.start();
		T1 t2 = new T1("T2");
		t2.start();
	}

}
