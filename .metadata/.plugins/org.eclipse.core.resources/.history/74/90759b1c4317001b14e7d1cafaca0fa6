package com.sync;

public class Bank {

	public static void main(String[] args) {
		Account acc = new Account(3000);
		Wth wt = new Wth(acc);
		Dth dt = new Dth(acc);
		wt.start();
		dt.start();
		Wth wt2 = new Wth(acc);
		Dth dt2 = new Dth(acc);
		wt2.start();
		dt2.start();
		Wth wt3 = new Wth(acc);
		Dth dt3 = new Dth(acc);
		wt3.start();
		dt3.start();
		System.out.println(acc);
	}

}
