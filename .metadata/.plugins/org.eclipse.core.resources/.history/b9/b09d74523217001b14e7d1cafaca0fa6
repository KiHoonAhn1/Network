package com.thread;

class Th1 extends Thread {
	String name;
	public Thread1() {}
	public Thread1(String name) {
		this.name = name;
	}
	@Override
	public void run() {
	}
	
}

public class Test5 {

	public static void main(String[] args) { // 누가 더 많은 점유율을 가지고 동작하는지는 OS가 결정
		T t1 = new T("T1");
		t1.start();
		T t2 = new T("T2");
		t2.start();
	}

}
