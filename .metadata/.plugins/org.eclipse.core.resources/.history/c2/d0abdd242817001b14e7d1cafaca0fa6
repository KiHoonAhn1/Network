package com.thread;

class Th extends Thread {

	String name;
	
	public Th() {}
	public Th(String name) {
		this.name = name;
	}
	
	@Override
	public void run() { //source -> override -> run
		for(int i=0;i<=100;i++) {
			System.out.println(name+":"+i);
			try {
				Thread.sleep(200);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}
}

public class Test {

	public static void main(String[] args) { // 누가 더 많은 점유율을 가지고 동작하는지는 OS가 결정
		Th t1 = new Th("T1");
		t1.start();
		Th t2 = new Th("T2");
		t2.start();
	}

}
